{
  "name": "@amityco/asc-react-native-ui-kit",
  "version": "3.0.0",
  "description": "test",
  "main": "lib/commonjs/index.js",
  "module": "lib/module/index.js",
  "types": "lib/typescript/src/index.d.ts",
  "react-native": "src/index.tsx",
  "source": "src/index",
  "files": [
    "src",
    "lib",
    "assets",
    "!**/__tests__",
    "!**/__fixtures__",
    "!**/__mocks__",
    "android",
    "ios",
    "cpp",
    "*.podspec",
    "!lib/typescript/example",
    "!ios/build",
    "!android/build",
    "!android/gradle",
    "!android/gradlew",
    "!android/gradlew.bat",
    "!android/local.properties",
    "!**/.*"
  ],
  "scripts": {
    "prepare": "husky install",
    "test": "jest",
    "typecheck": "tsc --noEmit",
    "lint": "eslint \"**/*.{js,ts,tsx}\"",
    "prepack": "bob build",
    "release": "release-it",
    "example": "yarn --cwd example",
    "example-npm": "npm run --prefix example",
    "bootstrap": "yarn example && yarn install"
  },
  "keywords": [
    "react-native",
    "ios",
    "android"
  ],
  "repository": "https://github.com/topAmity/amityco-asc-react-native-ui-kit",
  "author": "topAmity <thanaphon@amity.co> (https://github.com/topAmity)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/topAmity/amityco-asc-react-native-ui-kit/issues"
  },
  "homepage": "https://github.com/topAmity/amityco-asc-react-native-ui-kit#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native-community/eslint-config": "^3.0.2",
    "@react-navigation/native": "^6.1.18",
    "@react-navigation/native-stack": "^6.11.0",
    "@release-it/conventional-changelog": "^5.0.0",
    "@types/jest": "^28.1.2",
    "@types/lodash.debounce": "^4.0.7",
    "@types/react": "^18.2.37",
    "@types/react-native": "0.70.0",
    "@types/react-native-video": "^5.0.14",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^8.0.0",
    "jest": "^28.1.1",
    "lint-staged": "^15.2.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "^0.72.3",
    "react-native-builder-bob": "^0.20.4",
    "react-native-safe-area-context": "^4.10.9",
    "react-native-svg": "^15.6.0",
    "release-it": "^15.0.0",
    "typescript": "^4.5.2"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": "eslint --cache --fix",
    "*.{json,md}": "prettier --write"
  },
  "resolutions": {
    "@types/react": "17.0.21"
  },
  "peerDependencies": {
    "@react-native-async-storage/async-storage": "^1.19.3",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@react-navigation/stack": "^6.3.29",
    "react": "*",
    "react-native": "*",
    "react-native-safe-area-context": "^4.7.2",
    "react-native-screens": "^3.25.0",
    "react-native-svg": "^14.1.0"
  },
  "engines": {
    "node": ">= 16.0.0"
  },
  "packageManager": "^yarn@1.22.15",
  "jest": {
    "preset": "react-native",
    "modulePathIgnorePatterns": [
      "<rootDir>/example/node_modules",
      "<rootDir>/lib/"
    ]
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "eslintConfig": {
    "root": true,
    "extends": [
      "@react-native-community",
      "prettier"
    ],
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "quoteProps": "consistent",
          "singleQuote": true,
          "tabWidth": 2,
          "trailingComma": "es5",
          "useTabs": false
        }
      ]
    }
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "prettier": {
    "quoteProps": "consistent",
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      "typescript"
    ]
  },
  "dependencies": {
    "@amityco/ts-sdk-react-native": "^6.30.0",
    "@reduxjs/toolkit": "^1.9.7",
    "expo-asset": "^8.9.1",
    "expo-av": "^13.4.1",
    "expo-constants": "^14.2.1",
    "expo-image-picker": "^14.1.1",
    "expo-modules-core": "^1.5.4",
    "expo-video-thumbnails": "^7.2.1",
    "i18next": "22.4.10",
    "lodash.debounce": "^4.0.8",
    "moment": "2.29.4",
    "react-hook-form": "^7.49.3",
    "react-i18next": "12.1.5",
    "react-native-pager-view": "6.1.4",
    "react-native-paper": "^5.11.0",
    "react-native-progress": "^5.0.1",
    "react-native-radio-buttons-group": "^3.0.2",
    "react-redux": "^8.1.3",
    "redux": "^4.2.1",
    "use-memo-one": "1.1.3"
  }
}
